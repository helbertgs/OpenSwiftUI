{"primaryContentSections":[{"kind":"declarations","declarations":[{"tokens":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"EnvironmentKey"}],"languages":["swift"],"platforms":["macOS"]}]},{"kind":"content","content":[{"anchor":"overview","level":2,"type":"heading","text":"Overview"},{"type":"paragraph","inlineContent":[{"type":"text","text":"You can create custom environment values by extending the"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/EnvironmentValues"},{"type":"text","text":" structure with new properties."},{"type":"text","text":" "},{"type":"text","text":"First declare a new environment key type and specify a value for the"},{"type":"text","text":" "},{"type":"text","text":"required "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/EnvironmentKey\/defaultValue"},{"type":"text","text":" property:"}]},{"type":"codeListing","syntax":null,"code":["private struct MyEnvironmentKey: EnvironmentKey {","    static let defaultValue: String = \"Default value\"","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"The Swift compiler automatically infers the associated "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/EnvironmentKey\/Value"},{"type":"text","text":" type as the"},{"type":"text","text":" "},{"type":"text","text":"type you specify for the default value. Then use the key to define a new"},{"type":"text","text":" "},{"type":"text","text":"environment value property:"}]},{"type":"codeListing","syntax":null,"code":["extension EnvironmentValues {","    var myCustomValue: String {","        get { self[MyEnvironmentKey.self] }","        set { self[MyEnvironmentKey.self] = newValue }","    }","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"Clients of your environment value never use the key directly."},{"type":"text","text":" "},{"type":"text","text":"Instead, they use the key path of your custom environment value property."},{"type":"text","text":" "},{"type":"text","text":"To set the environment value for a view and all its subviews, add the"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/View\/environment(_:_:)"},{"type":"text","text":" view modifier to that view:"}]},{"type":"codeListing","syntax":null,"code":["MyView()","    .environment(\\.myCustomValue, \"Another string\")"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"As a convenience, you can also define a dedicated view modifier to"},{"type":"text","text":" "},{"type":"text","text":"apply this environment value:"}]},{"type":"codeListing","syntax":null,"code":["extension View {","    func myCustomValue(_ myCustomValue: String) -> some View {","        environment(\\.myCustomValue, myCustomValue)","    }","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"This improves clarity at the call site:"}]},{"type":"codeListing","syntax":null,"code":["MyView()","    .myCustomValue(\"Another string\")"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"To read the value from inside "},{"type":"codeVoice","code":"MyView"},{"type":"text","text":" or one of its descendants, use the"},{"type":"text","text":" "},{"type":"reference","isActive":true,"identifier":"doc:\/\/OpenSwiftUI\/documentation\/OpenSwiftUI\/Environment"},{"type":"text","text":" property wrapper:"}]},{"type":"codeListing","syntax":null,"code":["struct MyView: View {","    @Environment(\\.myCustomValue) var customValue: String","","    var body: some View {","        Text(customValue) \/\/ Displays \"Another value\".","    }","}"]}]}],"schemaVersion":{"major":0,"minor":2,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/openswiftui\/environmentkey"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"url":"doc:\